<%= page_title "Flugzeug anlegen" %>

<% form_for(@plane) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :kennzeichen %><br />
    <%= f.text_area :kennzeichen %>
  </p>
  <p>
    <%= f.label :verein %><br />
    <%= f.text_field :verein %>
  </p>
  <p>
    <%= f.label :typ %><br />
    <%= f.text_area :typ %>
  </p>
  <p>
    <%= f.label :sitze %><br />
    <%= f.text_field :sitze %>
  </p>
  <p>
    <%= f.label :gattung %><br />
    <%= f.text_area :gattung %>
  </p>
  <p>
    <%= f.label :wettbewerbskennzeichen %><br />
    <%= f.text_area :wettbewerbskennzeichen %>
  </p>
  <p>
    <%= f.label :bemerkung %><br />
    <%= f.text_area :bemerkung %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', planes_path %>
